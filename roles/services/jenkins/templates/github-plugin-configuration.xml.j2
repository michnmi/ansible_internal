<?xml version='1.1' encoding='UTF-8'?>
<github-plugin-configuration plugin="github@1.30.0">
  <configs>
{% for credential in jenkins_secret_text_credentials %}
    <github-server-config>
      <name>{{ credential['name'] }}</name>
      <apiUrl>https://api.github.com</apiUrl>
      <manageHooks>false</manageHooks>
      <credentialsId>{{ credential['name'] }}</credentialsId>
      <clientCacheSize>20</clientCacheSize>
    </github-server-config>
{% endfor %}
  </configs>
